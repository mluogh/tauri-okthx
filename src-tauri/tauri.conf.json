{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "tauri-macos-spotlight-app",
  "version": "0.2.0",
  "identifier": "com.tauri.spotlight",
  "plugins": {},
  "app": {
    "windows": [
      {
        "label": "overlay",
        "fullscreen": false,
        "transparent": true,
        "height": 250,
        "width": 800,
        "resizable": false,
        "decorations": false,
        "center": true,
        "visible": false,
        "acceptFirstMouse": true
      },
      {
        "label": "chat",
        "fullscreen": false,
        "alwaysOnTop": true,
        "height": 250,
        "width": 800,
        "resizable": false,
        "decorations": false,
        "center": false,
        "visible": false,
        "acceptFirstMouse": true,
        "url": "index.html/#chat"
      }
    ],
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true
  }
}
