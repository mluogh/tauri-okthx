{
  "identifier": "default",
  "description": "Capability for the chat window. NOTE: if you update tauri v2, need to prefix with core: and stuff",
  "local": true,
  "windows": ["chat", "main", "overlay"],
  "permissions": [
    "app:default",
    "window:allow-set-cursor-icon",
    "window:allow-start-dragging",
    "window:deny-internal-toggle-maximize",
    "event:allow-listen",
    "event:allow-unlisten",
    "window:allow-inner-size",
    "window:allow-set-size"
  ]
}
